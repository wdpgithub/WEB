/**首页css样式**/
page{
  height: 100%;
  background: #000;
}
.container{
  width: 100%;
  display: flex; /*小程序对弹性盒模型支持友好*/
  flex-direction: column;
  align-items: center;
  box-sizing: border-box; /*添加内间距不撑开*/
  cursor: pointer;
}

/*logo图片*/
.logo{
  width: 700rpx;  
  height: 579rpx;
  margin-top: 100rpx;
  margin-bottom: 150rpx;
}
.keep image{
  width: 658rpx;
  height: 200rpx;
}